#include<stdio.h>
#include<iostream>
using namespace std;

int main()
{
    int a = 0, b = 0, c = 0;
    while (cin>>a)
    {
        b = b + a;
        if (b < 0)
        {
           b = 0;
        }
        if (b> c)
        {
            c = b;
        }
        if (getchar() == '\n')
        {
            break;
        }
    }
    cout<< c;
    return 0;
}
